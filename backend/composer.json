{
  "name": "dexterlan/test-andata",
  "type": "project",
  "description": "Test task",
  "require": {
    "php": "^8.0",
    "aura/router": "^3.2",
    "doctrine/doctrine-orm-module": "^5.2",
    "laminas/laminas-config-aggregator": "^1.9",
    "laminas/laminas-diactoros": "^2.17",
    "laminas/laminas-servicemanager": "^3.16",
    "laminas/laminas-session": "^2.13",
    "psr/http-server-handler": "^1.0",
    "roave/psr-container-doctrine": "^3.5",
    "symfony/cache": "^5.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^9"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true
  }
}
